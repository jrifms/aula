public class Pessoa {
    public String nome;
    public Integer idade;
    

    public static void main(String[] args){
        Pessoa[] pessoas = new Pessoa[2];
        
        for(int i=0; i < 2; i++) {
        	pessoas[i] = new Pessoa();
        }

        pessoas[0].nome  = "Ana";
        pessoas[0].idade = 20;

        pessoas[1].nome = "Joao";
        pessoas[1].idade = 20;
        
    }
}
